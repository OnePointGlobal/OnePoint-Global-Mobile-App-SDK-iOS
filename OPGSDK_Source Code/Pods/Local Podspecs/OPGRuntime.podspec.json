{
  "name": "OPGRuntime",
  "version": "0.1.1",
  "summary": "A short description of Runtime Library.",
  "description": "Runtime SDK to take mobile surveys",
  "homepage": "https://github.com/OnePointGlobal/OPGRuntime",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "manjunath.ramesh@onepointglobal.com": "manjunath.ramesh@onepointglobal.com"
  },
  "source": {
    "git": "https://github.com/OnePointGlobal/OPGRuntime.git",
    "tag": "0.1.1"
  },
  "platforms": {
    "ios": "8.0"
  },
  "ios": {
    "vendored_libraries": "lib/libOnePoint.Runtime.a"
  },
  "preserve_paths": "libOnePoint.Runtime.a",
  "source_files": "include/OnePoint.Runtime/*.h",
  "public_header_files": "include/OnePoint.Runtime/*.h",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "/usr/include/libxml2",
    "OTHER_LDFLAGS": "-lxml2 -lz -ObjC",
    "LIBRARY_SEARCH_PATHS": "$(PODS_ROOT)/Pods/**"
  },
  "libraries": [
    "xml2",
    "c++",
    "iconv",
    "z"
  ]
}
